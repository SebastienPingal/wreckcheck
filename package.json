{
  "name": "nuxtjs-fastapi",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "fastapi-dev": "mkdir -p .venv && python -m venv .venv && .venv/bin/pip install -r requirements.txt && .venv/bin/python -m uvicorn api.index:app --reload",
    "nuxt-dev": "nuxt dev",
    "dev": "concurrently \"pnpm run nuxt-dev\" \"pnpm run fastapi-dev\"",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.7.0",
    "nuxt": "^3.16.2",
    "shadcn-nuxt": "2.0.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.4",
    "@vueuse/core": "^13.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^8.2.2",
    "lucide-vue-next": "^0.488.0",
    "reka-ui": "^2.2.0",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.1.4",
    "tw-animate-css": "^1.2.5"
  }
}
